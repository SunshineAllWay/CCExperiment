class InstanceInfo ( object ) : def __init__ ( self , connection = None , id = None , state = None ) : self . connection = connection self . id = id self . state = state def __repr__ ( self ) : return 'InstanceInfo:%s' % self . id def startElement ( self , name , attrs , connection ) : return None def endElement ( self , name , value , connection ) : if name == 'instanceId' or name == 'InstanceId' : self . id = value elif name == 'state' : self . state = value else : setattr ( self , name , value )
