""" Represents an EC2 Placement Group """ from boto . ec2 . ec2object import EC2Object from boto . exception import BotoClientError class PlacementGroup ( EC2Object ) : def __init__ ( self , connection = None , name = None , strategy = None , state = None ) : EC2Object . __init__ ( self , connection ) self . name = name self . strategy = strategy self . state = state def __repr__ ( self ) : return 'PlacementGroup:%s' % self . name def endElement ( self , name , value , connection ) : if name == 'groupName' : self . name = value elif name == 'strategy' : self . strategy = value elif name == 'state' : self . state = value else : setattr ( self , name , value ) def delete ( self ) : return self . connection . delete_placement_group ( self . name )
