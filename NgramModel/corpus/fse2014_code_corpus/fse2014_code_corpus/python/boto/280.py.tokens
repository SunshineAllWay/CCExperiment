class Attributes ( dict ) : def __init__ ( self , parent ) : self . parent = parent self . current_key = None self . current_value = None def startElement ( self , name , attrs , connection ) : pass def endElement ( self , name , value , connection ) : if name == 'Attribute' : self [ self . current_key ] = self . current_value elif name == 'Name' : self . current_key = value elif name == 'Value' : self . current_value = value else : setattr ( self , name , value )
