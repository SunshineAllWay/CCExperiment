class SecurityGroup ( object ) : def __init__ ( self , connection = None ) : self . name = None self . owner_alias = None def __repr__ ( self ) : return 'SecurityGroup(%s, %s)' % ( self . name , self . owner_alias ) def startElement ( self , name , attrs , connection ) : pass def endElement ( self , name , value , connection ) : if name == 'GroupName' : self . name = value elif name == 'OwnerAlias' : self . owner_alias = value
